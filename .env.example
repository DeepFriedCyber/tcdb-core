# TCDB API Configuration
# Copy this file to .env and customize as needed

# Feature Flags
# Enable TCDB addons (TDA, Fisher-Rao, LGT demos)
TCDB_ADDONS=false

# API Settings
TCDB_API_TITLE="TCDB API"
TCDB_API_VERSION="1.0.0"
TCDB_API_DESCRIPTION="Topological Computation Database API"

# Database Settings
TCDB_DATABASE_URL="sqlite:///./tcdb.db"

# Server Settings
TCDB_HOST="127.0.0.1"
TCDB_PORT=8000
TCDB_RELOAD=false

